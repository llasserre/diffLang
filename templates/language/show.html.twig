{% extends 'base.html.twig' %}

{% block title %}Language{% endblock %}

{% block nav %}
        {{ parent() }}
        <div style="position:absolute !important; left:10px !important;">
            <a href="{{ path('commande_index') }}"><img src="https://upload.wikimedia.org/wikipedia/commons/thumb/6/68/Feedbin-Icon-left-arrow.svg/626px-Feedbin-Icon-left-arrow.svg.png" height="50"></a>
        </div>
    </nav>
{% endblock %}
{% block body %}
    <div class="row">
        <div class="col-sm-6 text-center">
            <h1>Language</h1>
            <table class="table">
                <tbody>
                    <tr>
                        <th>Id</th>
                        <td>{{ language.id }}</td>
                    </tr>
                    <tr>
                        <th>Nom</th>
                        <td>{{ language.nom }}</td>
                    </tr>
                    <tr>
                        <th>Descriptif</th>
                        <td>{{ language.descriptif|e("html") }}</td>
                    </tr>
                </tbody>
            </table>
        </div>
        <div class="col-sm-6 text-center">
            {% if language.nom == "PHP" %}
                <a href="https://www.php.net/">
                    <img src="https://lh3.googleusercontent.com/proxy/6aiib2lVlAW8wQuzSTMlurUiQklt9RdltF7uXi5g5chLuqeefp84_og56fwOSm4q-NqxswPvF8PA4Vsvz3__sMBhrLN13-puSlQDUVVZBHHz" style="width:385px; height:268px"></img>
                </a>
            {% elseif language.nom == "JavaScript" %}
                <a href="https://developer.mozilla.org/fr/docs/Web/JavaScript">
                    <img src="https://dejanstojanovic.net/media/131814/16x9.png" style="width:385px; height:268px"></img>
                </a>
            {% elseif language.nom == "Java" %}
                <a href="https://docs.oracle.com/javase/tutorial/getStarted/index.html">
                    <img src="data:image/jpeg;base64," style="width:385px; height:268px"></img>
                </a>
            {% elseif language.nom == "C++" %}
                <a href="https://docs.microsoft.com/fr-fr/cpp/?view=msvc-160">
                    <img src="https://upload.wikimedia.org/wikipedia/commons/thumb/1/18/ISO_C%2B%2B_Logo.svg/1200px-ISO_C%2B%2B_Logo.svg.png" style="width:385px; height:268px"></img>
                </a>
            {% endif %}
        </div>
    </div>
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-6">
                <h1>Procédural</h1>
                {% for commande in language.commandes %}
                    {% if commande.typeCommande.typeCmdFamily.id == 1 %}
                        <div class="card">
                            <div class="card-header"> {{ commande.typeCommande.libelle }}</div>
                            <div class="card-body"> {{ commande.ligne }}</div>
                        </div>
                    {% endif %}
                {% endfor %}
            </div>
            <div class="col-sm-6">
                <h1>Orienté objet</h1>
                {% for commande in language.commandes %}
                    {% if commande.typeCommande.typeCmdFamily.id == 2 %}
                        <div class="card">
                            <div class="card-header"> {{ commande.typeCommande.libelle }}</div>
                            <div class="card-body"> {{ commande.ligne }}</div>
                        </div>
                    {% endif %}
                {% endfor %}
            </div>
        </div>
    </div>
    {# <a href="{{ path('language_edit', {'id': language.id}) }}">edit</a> #}

    {# {{ include('language/_delete_form.html.twig') }} #}
{% endblock %}
